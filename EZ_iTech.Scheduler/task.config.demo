<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="jobSettingSection" type="EZ_iTech.Scheduler.JobSettingSection,EZ_iTech.Scheduler"/>
  </configSections>
  <jobSettingSection>
    <jobSettings>
      <!--
      name:作业名称,
      description:作业描述,
      interval:作业的执行间隔，允许多个间隔。例如：13:15~11:16,13:18~11:20,13:12~11:13,
      count:作业需要的占用的资源(线程个数)      
      -->
      <jobSetting name="Job_1" interval="5" count="3" description="作业1，分配三个线程，每五秒执行一次" />
    </jobSettings>
  </jobSettingSection>
</configuration>